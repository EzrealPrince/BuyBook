.search{
  background-color: #666666;
  height: 90rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
}
/* .search-img{
  margin: auto 0 auto 20rpx;
} */
.search input{
  height: 90%;
  width: 96%;
  /* margin-left: 20px; */
  font-size: 28rpx;
  background-color: #fff;
  border-radius: 40rpx;
  margin: 1rpx auto 0;
}
.placeholder{
  font-size: 14px;
  color: #d1d1d1;
  margin-left: 45%;
}
swiper{
  width: 100%;
  height: 355rpx;
}
swiper image{
  width: 100%;
  height: 355rpx;
}
.thumbnail{
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 180rpx;
  margin-bottom: 40rpx;
  background-color: #fff;
  border-bottom: 1px solid #ededed;
}
.thumbnail-items{
  display: flex;
  flex-direction: column;
  align-items: center;
}
 .thumbnail-items image{
  vertical-align: center;
  margin: 20rpx 49rpx 8rpx;  
  width: 90rpx;
  height: 90rpx;
} 
.s-list{
  width: 100%;
  height: 550rpx;
  margin-bottom: 20rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  background-color: #fff;
}
 .s-items{
  display: flex;
  flex-direction: column;
} 
.s-items image{
  width: 375rpx;
  height: 372rpx;
  margin: 0 0 10rpx 0;
}
.s-items text{
  margin: 2px 5px;
}
.price{
  color: #ff4d4d;
}